{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers":[
            "last 5 versions",
            "ie >= 9"
          ],
          "node": "current"
        } 
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    ["@babel/plugin-transform-arrow-functions", { "spec": true }]
  ]
}